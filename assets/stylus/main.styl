/*Responsive Design*/
@media screen and (max-width:1366px)
  #inicio
    padding 40px 0 160px 0
    h1
      font-size 60px
      
  #topnav
    height 80px
    margin-bottom 100px
    ul
      height 80px
      
  #main_nav li a
    line-height 80px
    font-size 12px
    
  #main-content
    width 100%
    h2
      font-size 37px
    h3
      font-size 21px
      
  .section article
    padding 0 0 5em 0
    
  .title-proyecto
    max-width 740px
    
  .list.mailing-list li
    padding-bottom 1.2em
    a
      font-size 21px
    strong
      display inline-block
      font-weight normal
      font-size 21px
      margin-right 28px

@media screen and (max-width:1024px)
  #main-content article
    max-width 400px
    
  #inicio
    text-align center

  .content-service
    max-width 500px
    
  .title-proyecto
    max-width 500px
    
  .publicaciones-cabecera
    padding 100px 0 150px 0
    
  .gallery-cell
    width 100%
    background-size 110% !important
    background-position 90px !important
    height 320px
    
  .gallery-dev, .gallery-seo, .gallery-pro
    width 100%
    background-size 40% !important
    background-position 90% !important
    
  #publicaciones img
    opacity 1
    
  #cont-logo
    width 100%
    text-align center
    display block
    padding 15px 0 15px 0
    .logo
      position static
      max-width auto
  
  #informacion a
    display block
    margin-bottom 1em
    
      
@media screen and (max-width:768px)
  .logo
    position static
