html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video
  border 0
  box-sizing border-box
  font-size 100%
  margin 0
  outline 0
  padding 0
  text-rendering optimizeLegibility
  vertical-align top

li
  list-style none

@-webkit-viewport
  width device-width

@-moz-viewport
  width device-width

@-ms-viewport
  width device-width

@viewport
  width device-width

::-webkit-scrollbar
  width 15px

::-webkit-scrollbar-track
  background-color rgba(0,0,0,0.3)

::-webkit-scrollbar-thumb
  border-radius 10px
  background-color rgba(0,0,0,0.3)

/*::-moz-selection
  background transparent
  text-shadow none

::selection
  background transparent
  text-shadow none*/

//Paleta color
celeste-edgar01 = #36BDEC
gris-1 = #A0A0A0
gris-2 = #828282
gris-3 = #5A5A5A
gris-contra = #282828

img
  vertical-align middle

body
  -webkit-font-variant-ligatures no-common-ligatures
  background #FFFFFF
  font-family 'Share Tech',Helvetica,Arial,sans-serif
  font-size 20px
  font-variant-ligatures no-common-ligatures
  width 100%
  height 100%

html
  width 100%
  height 100%

a
  text-decoration none
  color gris-contra
  &:hover
    color celeste-edgar01

.visitar
  border solid 2px #FFFFFF !important
  display block
  font-size 21px
  margin 30px auto
  padding 15px 0 15px 0
  text-align center
  width 90%
  &:hover
    color #FFF !important

.anin-rapido
  transition all .2s ease-out

.anin-normal
  transition all .5s ease-out

.anin-lento
  transition all 1s ease-out

.chromeframe
  background #000
  color #fff
  height 100%
  padding-top 100px
  position fixed
  text-align center
  width 100%
  z-index 10000

//Portada
#portada
  width 100%
  height 100%
  text-align center
  display flex
  justify-content center
  background url("../img/flecha-abajo.png") no-repeat
  background-position center 90%
  animation pulse2 3s ease infinite
  figure
    align-self center
    animation pulse 2s ease-in infinite alternate
    img 
      max-width 80%
      
//Inicio
#inicio
  height auto
  padding 70px 0 70px 0
  width 100%
  hgroup
    margin 0 auto
    max-width 1300px
    padding-bottom 100px
  h1
    color gris-3
    font-size 90px
    line-height 1.1em
    padding-bottom 0.3em
    a
      color gris-2
      &:hover
        color gris-contra
  h2
    color gris-1
    font-size 67px
  p
    display inline-block
    padding-top 55px
    padding-left 25px
    font-size 1.2em
  
#logo-c
  padding 40px 0 80px 0
  display inline-block
  
//Menu Principal
#topnav
  height 48px
  margin-bottom 40px
  text-align center
  width 100%
  ul
    display flex
    height 48px
    margin 0 auto
    max-width 1300px

#main_nav
  li
    width 20%
    a
      cursor pointer
      font-size 1.2em
      line-height 48px
      letter-spacing 5px
      text-transform uppercase
      &:hover
        font-weight bold
        letter-spacing 2px

//Espacio Promoción
#promo
  background celeste-edgar01
  color #FFF
  .aviso-promo
    display block
    font-size 27px
    margin 0 auto
    max-width 750px
    text-align justify
  .title-promo
    text-align center
    font-size 50px
    line-height: 0.9 em
    padding 20px 0 50px 0
  strong
    font-weight lighter
  a
    color #FFF
    text-decoration underline


/*burbuja*/
#burbuja
  background #FFF
  padding 10px
  border-radius 3px
  position fixed
  top 10px
  right 10px
  p
    line-height 1.5em
    font-size 1.1em

/*Marcas*/
#marcas
  div
    display flex
    text-align center
  figure
    width 25%
  figcaption
    text-align center
    margin-top 1.3em
    a
      font-size 1.1em
      padding 10px
      &:hover
        border-radius 24px
        border 2px solid celeste-edgar01
        color celeste-edgar01
  img
    max-height 150px
    width auto/9
    filter grayscale(100%)
    &:hover
      filter grayscale(0%)
   
#promo article, #marcas article, #contacto article
  margin 0 auto
  max-width 1300px
  padding 130px 0 130px 0
  width 90%

#promo h3, #marcas h3, #top-post h3, #contacto h3
  color gris-2
  font-size 48px
  padding-bottom 80px
  text-align center
  
/*Publicaciones Principales*/
#top-post
  article
    margin 0 auto
    max-width 1300px
    padding 100px 0 100px 0
    width 90%
  p
    font-size 21px
  a
    border 2px solid gris-1
    border-radius 24px
    display inline-block
    margin 10px 10px
    padding 10px
    &:last-child
      background celeste-edgar01
      color #000
      &:hover
        background none
        color celeste-edgar01
    &:hover
      border 2px solid celeste-edgar01
      color celeste-edgar01

/*Contacto*/
#contacto
  //border-top 2px solid gris-1

#main-content > #contacto
  border none
  
#social
  margin-bottom 3em
  margin-top 2em
  a
    background url("../img/icon-logo.png") rgba(0,0,0,0.2)
    border 1px solid gris-2
    border-radius 3px
    color transparent
    display inline-block
    font-size 0
    height 48px
    margin-right 10px
    text-indent -99999999px
    width 48px
    &:nth-child(1)
      background-position -10px 0
    &:nth-child(2)
      background-position -73px 0
    &:nth-child(3)
      background-position -138px 0
    &:nth-child(4)
      background-position -200px 0
    &:nth-child(5)
      background-position -265px 0
    &:nth-child(6)
      background-position -333px 0
    &:nth-child(7)
      background-position -390px 0
    &:nth-child(8)
      background-position -455px 0
    &:nth-child(9)
      background-position -519px 0
    &:hover
      background-color celeste-edgar01
      transform rotate(360deg)

.list
  &.mailing-list
    li
      height auto
      padding-bottom 2em
      width 90%
      a
        border 2px solid gris-2
        border-radius 28px
        font-size 21px
        padding 10px
        &:hover
          border 2px solid celeste-edgar01
          color celeste-edgar01
    strong
      display inline-block
      font-size 21px
      font-weight normal
      width 110px
      
/*Pie*/
footer
  font-size 1.2em
  height auto
  margin 0 auto
  max-width 1280px
  padding 2em 0 2em 0
  text-align right
  width 100%

/*Pagina Presentacion*/
.logo
  height auto
  left 20px
  max-width 5%
  position fixed
  top 10px
  width auto/9
  &.scroll
    left 125px
    top 40px

/*Menu otras paginas*/
#topnav2
  height 80px
  width 100%
  ul
    display flex
    height 80px
    margin 0 auto
    max-width 1024px

#main_nav2
  li
    width 20%
    a
      cursor pointer
      display block
      font-size 1em
      line-height 80px
      text-align center
    &.change a
      color celeste-edgar01
    :hover
      color celeste-edgar01
    .selector
      border-top solid 5px celeste-edgar01
      color celeste-edgar01
      font-size 1.2em

.selector
  &:after
    border-left 8px solid transparent
    border-right 8px solid transparent
    border-top 8px solid celeste-edgar01
    content ""
    display block
    margin -5px auto
    width 0
    

/*Titulo de pagina*/
#head-title
  margin 0 auto
  max-width 1200px
  padding-top 50px
  width 90%
  h1
    font-size 50px
    text-align center

/*Navegacion por pagina*/
#navigation-per-page
  left 50px
  margin-top 45px
  position fixed
  width 180px
  ul
    height auto
    width 100%
  li
    font-size 1.3em
    line-height 2.5em
    text-align right
  a
    &.current
      color celeste-edgar01
      &:after
        border-bottom 5px solid transparent
        border-left 5px solid celeste-edgar01
        border-top 5px solid transparent
        content ""
        display inline-block
        margin-left 5px
        width 0

.isStuck
  z-index 80

/*Paginas Secundarias*/
#main-content
  width 100%
  article
    margin 0 auto
    max-width 720px
    padding-top 1px
    width 90%
  h2
    border-left 10px solid celeste-edgar01
    font-size 50px
    margin 10px 0 50px 0
    padding-left 10px
  h3
    font-size 2.313em
    padding 1em
  figure
    padding 20px 0 20px 0
    text-align center
  img
    height auto
    max-width 90%
    width auto/9
  figcaption
    padding-top 1em

.text-full
  font-family 'Poppins', sans-serif
  font-size 1em
  font-weight normal
  line-height 1.2em
  margin-bottom 1em
  margin-top 1em
  text-align justify
  .link-out
    margin-bottom 1em
    display inline-block
  img
    display block
    margin 0 auto
    padding-top 1em
    padding-bottom 2em
    
.section
  margin 0 auto
  width 100%
  article
    margin 0 auto
    padding 4em 0 4em 0
    width 100%
  a
    border solid 2px celeste-edgar01
    border-radius 28px
    padding 10px
    transition all .5s ease-out
    &:hover
      color celeste-edgar01
      
.s3 a
  display inline-block
  font-size 1.313em
  margin-bottom .8em
  margin-right 1em
  padding 10px

/*Pagina de Publicaciones*/
#publicaciones
  padding 50px 5% 50px 5%
  display flex
  justify-content space-around
  div
    max-width 30%
    padding 10px
    h3
      font-size 37px
      text-align center
  a
    border-left solid 5px celeste-edgar01
    display block
    margin-bottom 20px
    padding 10px
    &:hover
      color celeste-edgar01
  p
    font-size 21px

.page-noticias
  .section
    &:nth-child(odd)
      background #FFF
      border-top solid 2px gris-1
    &:nth-child(even)
      background #FFF
      border-top solid 2px gris-1
      padding-top 50px

.publicaciones-cabecera
  height auto
  padding 40px 0 50px 0
  text-align center
  width 100%
  hgroup
    margin 0 auto
    max-width 90%
  h1
    font-size 67px
    padding-top 20px
  h2
    border 0 !important
    font-size 21px !important
    padding 0 !important
  .ed
    border solid 8px #B1B1B1
    border-radius 50%
    height 200px
    width 200px
  #autor a
    color #B1B1B1

.atras
  height 50px
  left 3%
  position fixed
  top 5%

.arriba
  bottom 5%
  height 50px
  position fixed
  right 3%

//busqueda
#busqueda
  margin 0 auto
  max-width 1024px

.text-recomendation
  font-size 28px
  font-style italic
  margin-bottom 1em

.social
  padding 30px 0 20px

#comentarios
  display block
  margin 0 auto
  max-width 1024px
  padding 30px 0 30px 0
  width 90%

#recomendation
  margin 0 auto
  max-width 1024px
  padding 25px 0 5px 0
  width 90%

.videoWrapper
  height 0
  padding-bottom 56.25%
  padding-top 25px
  position relative
  iframe
    height 100%
    left 0
    position absolute
    top 0
    width 100%

/*Portafolio*/
.content-service
